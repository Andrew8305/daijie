<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
<generatorConfiguration>  
    <classPathEntry  location="D:\apache-maven-3.2.5-bin\data\mysql\mysql-connector-java\5.1.9\mysql-connector-java-5.1.9.jar"/>
    <context id="recodecenter_db"  targetRuntime="MyBatis3">

        <!-- 由于beginningDelimiter和endingDelimiter的默认值为双引号(")，在Mysql中不能这么写，所以还要将这两个默认值改为**反单引号(`)**，配置如下：  -->
        <property name="beginningDelimiter" value=""/>
        <property name="endingDelimiter" value=""/>

        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>

        <!--<plugin type="plugin.serializable.SerializablePlugin"></plugin>-->

        <!-- page plugin && add deleteLogicByIds method  -->
        <!-- 单表逻辑的话， 可以增加下面的两个两个自定义插件，关联关系的话，建议注释掉如下的两个插件-->
        <!--<plugin type="plugin.PaginationPlugin"></plugin>-->
        <!--<plugin type="plugin.DeleteLogicByIdsPlugin"></plugin>-->

        <!--<plugin type="plugin.mutiDatasource.MutiDatasourcePaginationPlugin"></plugin>-->

        <commentGenerator>  
            <property name="suppressDate" value="true"/>   
            <property name="suppressAllComments" value="true"/>  
        </commentGenerator>  
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://10.0.4.1:3306/zima_user" userId="root" password="123456">  
        </jdbcConnection>  
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>   
        <javaModelGenerator targetPackage="com.gwy.recodecenter.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>  
            <property name="trimStrings" value="true"/>  
        </javaModelGenerator> 
        <sqlMapGenerator targetPackage="mybatis-mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>   
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.gwy.recodecenter.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>  
        </javaClientGenerator>
        <table tableName="user" domainObjectName="User" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
    </context>  
</generatorConfiguration>